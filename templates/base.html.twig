<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}DayOneVax{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_link_tags('bootstrap') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('bootstrap') }}
    {% endblock %}
</head>
<body>
{% block body %}

    {% block header %}
        {% include 'layout/header.html.twig' %}
    {% endblock %}

    {% block content %}
    {% endblock %}

    {% block footer %}
        {% include 'layout/footer.html.twig' %}
    {% endblock %}

{% endblock %}
</body>
</html>
